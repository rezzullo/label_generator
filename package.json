{
  "name": "labelgenerator",
  "displayName": "LabelGenerator",
  "description": "",
  "version": "1.0.2",
  "publisher": "rezzullo",
  "engines": {
    "vscode": "^1.91.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rezzullo/labelgenerator.git"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "LabelGenerator",
        "title": "Generate label"
      }
    ],
    "viewsContainers": {
            "activitybar": [
                {
                    "id": "labelGeneratorView",
                    "title": "Label Generator",
                    "icon": "images/la creazione di adamo versione 2.png" 
                }
            ]
        },
        "views": {
            "labelGeneratorView": [
                {
                    "id": "labelGeneratorView",
                    "name": "Label Generator"
                }
            ]
        },
    "keybindings": [
      {
        "command": "LabelGenerator",
        "key": "ctrl+shift+l"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "xml2js": "^0.6.2"
  },
  "files": [
    "out/webview.html",
    "src/extension.ts",
    "out"
  ]
}
